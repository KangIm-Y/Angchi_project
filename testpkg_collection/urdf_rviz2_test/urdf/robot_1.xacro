<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro" name="urdf_test" >

    <!-- BASE -->
    <link name="base_link">
    </link>

    <!-- BODY LINK -->
    <!-- body link is fuxing GaSang link -->
    <joint name="body_joint" type="fixed">
        <parent link="base_link"/>
        <child link="body"/>
    </joint>

    <!-- 1 meter by 1 by 1 box. -->
    <link name="body">
        <visual>
            <geometry>
                <box size="1 1 1"/>
            </geometry>
            <material name="white"/>
        </visual>
    </link>

</robot>